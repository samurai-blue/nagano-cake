

<h2>配送先編集</h2>




<%= form_with model: @shipping, url:"/shippings/#{@shipping.id}", local:true do |f| %>
  <div class="form-group">
    <%= f.label :"郵便番号" %>
    <%= f.text_field :postal_code %> <!--, class: 'form-control book_title'は必要？-->
  </div>
  <div class="form-group">
    <%= f.label :"住所" %>
    <%= f.text_field :address %> <!--, class: 'form-control book_title'は必要？-->
  </div>
  <div class="form-group">
    <%= f.label :宛名 %>
    <%= f.text_field :name %> <!--, class: 'form-control book_title'は必要？-->
  </div>
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>